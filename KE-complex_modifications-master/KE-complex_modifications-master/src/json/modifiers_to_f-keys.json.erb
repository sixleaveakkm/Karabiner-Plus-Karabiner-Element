{
    "title": "Tapping modifier-keys produces a f-key.",
    "rules": [
        {
            "description": "Press left_shift alone produces F12",
            "manipulators": [
                {
                    "type": "basic",
                    "from": <%= from("left_shift", [], ["any"]) %>,
                    "to": <%= to([["left_shift"]]) %>,
                    "to_if_alone": <%= to([["f12"]]) %>
                }
            ]
        },
